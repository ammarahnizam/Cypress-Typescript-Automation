{
  "compilerOptions": {
    "outDir": "lib",
    "strict": true,
    "target": "es6",
    "module": "es6",
    "lib": ["ES2020", "dom"],
    "allowJs": true,
    "noImplicitAny": false,
    "declaration": false,
    "sourceMap": true,
    "inlineSourceMap": false,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "pretty": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "types": ["cypress"]
  },
  "include": ["**/*.ts"]
}
